package com.kh.variable.pre;

import com.kh.variable.ex.EnumEx.날짜들;

public class EnumPre {
	public enum 무지개 {
		빨,
		주,
		노,
		초,
		파,
		남,
		보
	}
	public static void main(String[] args) {
		System.out.println("빨강 : " + 무지개.빨);
		System.out.println("주황 : " + 무지개.주);
		System.out.println("노랑 : " + 무지개.노);
		System.out.println("초록 : " + 무지개.초);
		System.out.println("파랑 : " + 무지개.파);
		System.out.println("남색 : " + 무지개.남);
		System.out.println("보라색 : " + 무지개.보);
	}
	public enum 계절 {
		봄,여름,가을,겨울
		
	}

	
}
